<ul id="set-{name}">
{loop tags}
<li id="tag-{loop/_value}"><a href="{site/prefix}/sitetag-app/set.{name}/tag.{loop/_value}">{loop/_value}</a>{if obj.canEdit} <small><a href="#" onclick="tag.del ('{loop/_value}')">x</a></small>{end if}</li>
{end loop}
</ul>
{if obj.canTag}
<form id="add-tag-form" onsubmit="return tag.add (this)">
<input type="text" id="taginput" name="taginput" value="" autocomplete="off" />
<input type="submit" value="{intl Add}" />
</form>
{end if}
